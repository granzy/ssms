<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mappers.user.UserStatusHistoryMapper">
    <cache />

    <select id="getLast" resultType="UserStatusHistory">
        SELECT
        * FROM sys_user_status_history WHERE user_id = #{userId}
        ORDER by sys_user_status_history.op_date desc limit 1
    </select>
</mapper>